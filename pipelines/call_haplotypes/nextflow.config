// docker
docker.enabled = true

// s3
plugins {
  id "nf-amazon"
}

// launch info
trace {
  enabled = true
  file = "$PWD/info/trace.txt"
}
timeline {
  enabled = true
  file = "$PWD/info/timeline.html"
}
dag {
  enabled = true
  file = "$PWD/info/dag.html"
}

// launch profiles
profiles {
  local {
    process.executor = "local"
  }
  awsbatch {
    process.executor = "awsbatch"
    process.region = "ap-southeast-1"
  }
}